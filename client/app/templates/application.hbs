    <div class="container-fluid">
      <div class="row">
        <div class="col-md-offset-2 col-sm-8 col-md-8 main">
          <h2>Add New Game</h2>
          <form class="form-inline">
            <div class="form-group">
              <label for="rank">Rank</label>
              <input type="rank" class="form-control" id="rank" size="4" maxlength="4">
            </div>
            <div class="form-group">
              <div class="btn-group">
                <button type="button" class="btn btn-primary">Win</button>
                <button type="button" class="btn btn-primary">Loss</button>
                <button type="button" class="btn btn-primary">Draw</button>
              </div>
            </div>
            <button type="submit" class="btn btn-default">Save</button>
          </form>
          <br>
          <h2 class="page-header">Dashboard</h2>
          <div class="row placeholders">
            <div class="col-xs-6 col-sm-3 placeholder">
              <img src="http://placehold.it/200x200" width="200" height="200" class="img-responsive" alt="Generic placeholder thumbnail">
              <h4>Label</h4>
              <span class="text-muted">Something else</span>
            </div>
            <div class="col-xs-6 col-sm-3 placeholder">
              <img src="http://placehold.it/200x200" width="200" height="200" class="img-responsive" alt="Generic placeholder thumbnail">
              <h4>Label</h4>
              <span class="text-muted">Something else</span>
            </div>
            <div class="col-xs-6 col-sm-3 placeholder">
              <img src="http://placehold.it/200x200" width="200" height="200" class="img-responsive" alt="Generic placeholder thumbnail">
              <h4>Label</h4>
              <span class="text-muted">Something else</span>
            </div>
            <div class="col-xs-6 col-sm-3 placeholder">
              <img src="http://placehold.it/200x200" width="200" height="200" class="img-responsive" alt="Generic placeholder thumbnail">
              <h4>Label</h4>
              <span class="text-muted">Something else</span>
            </div>
          </div>
          <h2 class="sub-header">Season 4</h2>
          <div class="table-responsive">
                <table class="table table-striped">
                    <thead> 
                        <th>Game Number</th> 
                        <th>Game Time</th> 
                        <th>Rank</th> 
                        <th>Outcome</th> 
                    </thead> 
                    <tbody> 
                        {{#each model.games as |game|}}
                        <tr>
                        <td>{{game.id}}</td>  
                        <td>{{game.createdAt}}</td> 
                        <td>{{game.rank}}</td> 
                        <td>{{game.outcome}}</td>
                        </tr> 
                        {{/each}} 
                    </tbody> 
                </table> 
          </div>
        </div>
      </div>
    </div>

